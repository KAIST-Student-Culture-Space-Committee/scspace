    "build": "dotenv -e .env -- pnpm -r --stream --parallel run build",
    "local:docker-up": "docker-compose -f docker-compose.local.yml up -d --build",
    "local:docker-down": "docker-compose -f docker-compose.local.yml down",
    "local:docker-clear": "docker-compose -f docker-compose.local.yml down -v",
    "prod:docker-up": "docker-compose -f docker-compose.prod.yml up -d",
    "prod:docker-down": "docker-compose -f docker-compose.prod.yml down",
    "prod:docker-clear": "docker-compose -f docker-compose.prod.yml down -v",
    "migrate": "drizzle-kit migrate --config packages/server/src/db/drizzle.config.ts",
    "generate": "drizzle-kit generate --config packages/server/src/db/drizzle.config.ts",
    "push": "drizzle-kit push --config packages/server/src/db/drizzle.config.ts"
  },
